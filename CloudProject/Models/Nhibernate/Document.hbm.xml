<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="CloudProject" namespace="CloudProject.Models">
  <class name="Document" table="SingleDocument" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="ID" column="ID" type="int">
      <generator class="native" />
    </id>
    <property name="Name" />
    <property name="Date" />
    <property name="Author" />
    <property name="Link" />
  </class>
</hibernate-mapping>
